
<tal:block define="global portal_url python:here.portal_url(relative=1)" />
<tal:block repeat="att python:['', 'Att', 'Dirty']">
  <tal:block repeat="status python:['confirmed', 'unconfirmed', 'cancelled']">
td.week<tal:block replace="att" /><tal:block replace="status" /> {
  background-image: url(<tal:block replace="portal_url" />/calWeek<tal:block replace="att" /><tal:block replace="status" />.png);
  background-repeat: no-repeat;
}
  </tal:block>
</tal:block>

.calMaigre {  font-family: Verdana, Arial, Helvetica, sans-serif; font-size: 75%; color: #000000; text-decoration: none}

small.calEvent {
  color: white
}

a.calEvent {
  color: white
}

a.calEvent:link {
  color: white
}

a.calEvent:visited {
  color: white
}

a.calEvent:active {
  color: #DDDDDD
}

a.calEvent:hover {
  color: white
}

th.dayEvent {
  background-color: black;
  text-align: left;
}

img.dayIcon {
  vertical-align:middle
}

td.calLeftTab {
  vertical-align:middle;
  color: white;
  text-align: center;
  background-image: url(<tal:block replace="portal_url" />/cal_left_tab.png);
}

td.calCenterTab {
  vertical-align:middle;
  color: white;
  text-align: center;
  background-image: url(<tal:block replace="portal_url" />/cal_center_tab.png);
}

td.calRightTab {
  vertical-align:middle;
  color: white;
  text-align: center;
  background-image: url(<tal:block replace="portal_url" />/cal_right_tab.png);
}

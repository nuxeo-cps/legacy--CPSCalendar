<tal:block define="global current_action string:meeting" />
<html metal:use-macro="here/main_template/macros/master">

<metal:block fill-slot="head_slot"
             i18n:translate="cpscalendar_heading_meeting_creation_helper">
  Meeting creation helper
</metal:block>

<metal:block fill-slot="main">
  <span class="calTitle" i18n:translate="cpscalendar_message_your_meeting_request_has_errors">
    Your request for a meeting has errors
  </span>:
  <ul>
    <li tal:repeat="error options/errors"
        tal:content="python:str(error)">
      Error
    </li>
  </ul>
  <input type="button" onClick="history.back()"
         value="button_back"
         i18n:attributes="value" />
</metal:block>

</html>
